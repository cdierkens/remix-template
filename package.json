{
  "name": "remix-template",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "main": "app.js",
  "scripts": {
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build": "yarn build:css && remix build",
    "check:lint": "eslint .",
    "check:types": "tsc",
    "deploy": "yarn pulumi up",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "dev": "run-p \"dev:*\"",
    "pulumi": "pulumi --cwd infrastructure",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "test:unit": "vitest",
    "test:component": "cypress run --component",
    "test:e2e": "cypress run",
    "ci:e2e": "cross-env NODE_ENV=test start-server-and-test dev http://localhost:8080 test:e2e",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "@react-aria/utils": "^3.15.0",
    "@remix-run/express": "^1.13.0",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/server-runtime": "^1.13.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "firebase": "^9.17.1",
    "isbot": "^3.6.6",
    "morgan": "^1.10.0",
    "prisma": "^4.10.1",
    "react": "^18.2.0",
    "react-aria": "^3.23.0",
    "react-date-picker": "^9.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-stately": "^3.21.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "remix-validated-form": "^4.6.9",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@pulumi/docker": "^3.6.1",
    "@pulumi/gcp": "^6.50.0",
    "@pulumi/pulumi": "^3.55.0",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react": "^3.1.0",
    "archiver": "^5.3.1",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.7.0",
    "daisyui": "^2.51.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "start-server-and-test": "^1.15.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,jsx}": "eslint --fix"
  }
}
